using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication2
{
    class Program
    {
        static void Main(string[] args)
        {
            int i = 1; int n;
            Console.WriteLine("Ent6er the number");
            n = int.perse(Console.ReadLine());

            Console.WriteLine("The table of given line is:");
            do
            {
             Console.WriteLine("" + ((n * i));
                i++;
            }
            while(i <= 10);
            Console.ReadLine();
        }
    }
}
